Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"determineQuadratic", "[", 
   RowBox[{"a_", ",", "b_", ",", "c_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "delta", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"delta", " ", "=", " ", 
      RowBox[{
       RowBox[{"b", "^", "2"}], "-", 
       RowBox[{"4", "*", "a", "*", "c"}]}]}], " ", ";", "\[IndentingNewLine]", 
     RowBox[{"Which", "[", 
      RowBox[{
       RowBox[{"delta", "<", "0"}], ",", 
       RowBox[{
       "Print", "[", "\"\<Brak rozwi\:0105za\:0144 rzeczywistych\>\"", "]"}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{"delta", "==", "0"}], ",", 
       RowBox[{
